{
  "expo": {
    "name": "LeARn",
    "slug": "LeARn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher.png",
    "scheme": "learn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.LeARn"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundImage": "./assets/images/ic_launcher_background.png",
        "monochromeImage": "./assets/images/ic_launcher_monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.LeARn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Poppins-BlackItalic.ttf",
            "./assets/fonts/Poppins-Bold.ttf",
            "./assets/fonts/Poppins-BoldItalic.ttf",
            "./assets/fonts/Poppins-ExtraBold.ttf",
            "./assets/fonts/Poppins-ExtraBoldItalic.ttf",
            "./assets/fonts/Poppins-ExtraLight.ttf",
            "./assets/fonts/Poppins-ExtraLightItalic.ttf",
            "./assets/fonts/Poppins-Italic.ttf",
            "./assets/fonts/Poppins-Light.ttf",
            "./assets/fonts/Poppins-LightItalic.ttf",
            "./assets/fonts/Poppins-Medium.ttf",
            "./assets/fonts/Poppins-MediumItalic.ttf",
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf",
            "./assets/fonts/Poppins-SemiBoldItalic.ttf",
            "./assets/fonts/Poppins-Thin.ttf",
            "./assets/fonts/Poppins-ThinItalic.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "firebase": {
        "apiKey": "AIzaSyD0Qov1Vx6ZqCNY1YwvZZpbcoHNlGEfKm8",
        "authDomain":"learn-9fd4e.firebaseapp.com",
        "projectId": "learn-9fd4e",
        "storageBucket":"learn-9fd4e.firebasestorage.app",
        "messagingSenderId":"462459983821",
        "appId": "1:462459983821:android:7ca5334da837715fb05684"
      }
    }
  }
}
